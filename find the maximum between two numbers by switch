#include <stdio.h>
int main()
{
   int a, b;
   scanf("%d %d", &a, &b);
   switch(a>=b)
   {
   case 1: //true
    {
        switch(a==b)
        {
        case 1:
            printf("both are equal");
            break;

        case 0:
            printf("a is max");
            break;
        }
        break;
        case 0: //false
            printf("b is max");
            break;
    }

   }

    return 0;
}
