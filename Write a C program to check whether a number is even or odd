#include <stdio.h>

int main()
{
    int num; //Write a C program to check whether a number is even or odd...

    printf("Enter any number: ");
    scanf("%d", &num);

    switch (num%2==0)
    {

    case 1:
        printf("%d is even.", num);
        break;

    case 0:
        printf("%d is odd.", num);
        break;
    }


    return 0;
}
