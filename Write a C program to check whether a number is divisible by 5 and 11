#include <stdio.h>

int main()
{
    int num; //Write a C program to check whether a number is divisible by 5 and 11...

    printf("Enter any number: ");
    scanf("%d", &num);

    switch ((num%5==0)&&(num%11==0))
    {

    case 1:
        printf("%d is divisible by 5 and 11.", num);
        break;

    case 0:
        printf("%d is not is divisible by 5 and 11.", num);
        break;
    }


    return 0;
}
